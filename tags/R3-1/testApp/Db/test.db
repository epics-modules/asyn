record(calc,"$(name):P$(port):A$(addr):calc") {
    field(DESC, "Counter")
    field(SCAN,"Passive")
    field(CALC, "(A<9)?(A+1):0")
    field(INPA,"$(name):P$(port):A$(addr):calc NPP NMS")
    field(FLNK,"$(name):P$(port):A$(addr):so")
    field(EGU, "Counts")
    field(HOPR, "10")
}
record(stringout,"$(name):P$(port):A$(addr):so") {
    field(DTYP,"asynTest")
    field(OUT,"@$(port),$(addr)")
    field(DOL,"$(name):P$(port):A$(addr):calc NPP NMS")
    field(OMSL,"closed_loop")
    field(FLNK,"$(name):P$(port):A$(addr):si")
}
record(stringin,"$(name):P$(port):A$(addr):si") {
    field(DTYP,"asynTest")
    field(INP,"@$(name):P$(port):A$(addr):so")
}
