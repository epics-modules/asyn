}
record(calc,"$(name)L$(L):srqCount") {
    field(DESC, "Count unsollicited SRQs")
    field(CALC, "A+B")
    field(INPA, "$(name)L$(L):srqCount")
    field(INPB,"1")
    field(EGU, "interrupts")
}

