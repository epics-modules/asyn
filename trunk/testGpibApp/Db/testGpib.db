}
record(stringout,"$(name)L$(L):putidnvalue") {
    field(DOL,"$(name)L$(L):idn")
    field(OUT,"$(name)L$(L):idnvalue PP")
    field(OMSL,"closed_loop")
}
record(stringin,"$(name)L$(L):idnvalue") {
}

