record(calc,"$(ioc):$(port)$(addr)traceCalc")
{
    field(INPA,"$(ioc):$(port)$(addr)traceError CP")
    field(INPB,"$(ioc):$(port)$(addr)traceIODevice CP")
    field(INPC,"$(ioc):$(port)$(addr)traceIOFilter CP")
    field(INPD,"$(ioc):$(port)$(addr)traceIODriver CP")
    field(INPE,"$(ioc):$(port)$(addr)traceFlow CP")
    field(CALC,"A+B*2+C*4+D*8+E*16")
    field(FLNK,"$(ioc):$(port)$(addr)traceSet")
    field(PINI,"YES")
}
record(longout,"$(ioc):$(port)$(addr)traceSet")
{
     field(DTYP,"asynDriver Trace")
     field(OUT,"@$(port),$(addr)")
     field(DOL,"$(ioc):$(port)$(addr)traceCalc")
     field(OMSL,"closed_loop")
}

